{
  "name": "qusly",
  "version": "0.1.0",
  "description": "A full-featured, elegant FTP client.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "fuse-box",
    "wexond",
    "ftp",
    "sftp",
    "client",
    "qusly",
    "nersent"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xNerhu/ftp-client.git"
  },
  "bugs": {
    "url": "https://github.com/xNerhu/ftp-client#issues"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" -&& tslint \"src/**/*.ts*\" --fix"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/gsap": "1.20.2",
    "@types/nedb": "1.8.8",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/styled-components": "4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "dotenv": "8.1.0",
    "electron": "6.0.5",
    "electron-builder": "21.2.0",
    "electron-ext-icon": "^1.0.1",
    "electron-updater": "^4.1.2",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "gsap": "^2.1.3",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "nedb": "^1.8.0",
    "prettier": "^1.18.2",
    "pretty-bytes": "^5.3.0",
    "qusly-core": "^1.1.9",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-windows-controls": "^1.1.1",
    "rectangle-selection": "^1.0.3",
    "styled-components": "^4.3.2",
    "ts-loader": "^6.0.4",
    "typescript": "3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
