{
  "name": "qusly",
  "version": "0.1.0",
  "description": "A full-featured, elegant FTP client.",
  "keywords": [
    "typescript",
    "react",
    "styled-components",
    "electron",
    "fuse-box",
    "wexond",
    "ftp",
    "sftp",
    "client",
    "qusly",
    "nersent"
  ],
  "author": {
    "name": "Miko≈Çaj Palkiewicz",
    "email": "xnerhu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xNerhu/ftp-client.git"
  },
  "bugs": {
    "url": "https://github.com/xNerhu/ftp-client#issues"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" -&& tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/gsap": "1.20.2",
    "@types/keytar": "^4.4.0",
    "@types/nedb": "^1.8.9",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.3",
    "dotenv": "8.1.0",
    "electron": "6.0.11",
    "electron-builder": "21.2.0",
    "electron-ext-icon": "^1.0.1",
    "electron-updater": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "gsap": "^2.1.3",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.14.0",
    "mobx-react-lite": "^1.4.1",
    "nedb": "^1.8.0",
    "prettier": "^1.18.2",
    "pretty-bytes": "^5.3.0",
    "qusly-core": "^2.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-windows-controls": "^1.1.1",
    "rectangle-selection": "^1.0.3",
    "resizable-box": "^1.0.0",
    "styled-components": "^4.4.0",
    "ts-loader": "^6.2.0",
    "typescript": "3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "keytar": "^5.0.0-beta.2"
  }
}
