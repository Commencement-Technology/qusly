{
  "appId": "org.qusly.qusly",
  "productName": "Qusly",
  "asar": true,
  "directories": {
    "output": "dist",
    "buildResources": "static/app-icons"
  },
  "linux": {
    "category": "FileTransfer",
    "target": [
      {
        "target": "deb",
        "arch": ["ia32", "x64"]
      },
      {
        "target": "AppImage",
        "arch": ["ia32", "x64"]
      }
    ]
  },
  "win": {
    "target": [
      {
        "target": "nsis-web",
        "arch": ["x64", "ia32"]
      },
      {
        "target": "zip",
        "arch": ["x64", "ia32"]
      }
    ]
  },
  "mac": {
    "category": "public.app-category.utilities"
  }
}
